snippet #! "Shebang for py.script" bA
#!/usr/bin/env python
$0
endsnippet

snippet ifmain "ifmain" b
if __name__ == '__main__':
	${1:main()}$0
endsnippet

snippet import "import module" b
import ${1:module}
endsnippet

snippet from "from module import name" b
from ${1:module} import ${2:Stuff}
endsnippet
